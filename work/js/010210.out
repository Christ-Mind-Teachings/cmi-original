    "uid": 102,
    "pid": 6,
    "text": "what"
  }
  ,{
    "discard": 1,
    "source": "nwffacim",
    "book": "2010",
    "bid": 11,
    "unit": "010210",
    "uid": 102,
    "pid": 7,
    "text": "thank you it so helpful"
  }
  ,{
    "discard": 1,
    "source": "nwffacim",
    "book": "2010",
    "bid": 11,
